   "source": [
    "import numpy as np \n",
    "A = np.array([[11,34,22],[14,15,51],[56,1,61]])\n",
    "M = np.mean(A.T, axis = 1)\n",
    "C = A-M\n",
    "V = np.cov(C)\n",
    "values, vectors = np.linalg.eig(V)\n",
    "P = vectors.dot(C)\n",
    "print(P)"
   ]
