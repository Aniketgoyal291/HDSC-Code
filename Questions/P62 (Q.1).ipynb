    "import numpy as np\n",
    "import scipy\n",
    "A = np.array([[1,0,0,3,0,2],\n",
    "             [2,0,0,0,5,0],\n",
    "             [0,0,2,0,4,0]])\n",
    "print(A)\n",
    "S = scipy.sparse.csr_matrix(A)\n",
    "print(S)\n",
    "sparcity = 1.0 - np.count_nonzero(A)/A.size\n",
    "print(sparcity)"
